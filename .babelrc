{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-transform-spread",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "esmodules": true
            }
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ]
    },
    "umd": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "umd"
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ]
    }
  }
}
